    ((satysfi "main.saty" "-o" "main.pdf")))
  (sources
    ((doc "example/main.saty" "example/main.saty")
     (doc "example/main.pdf" "example/main.pdf")
     (doc "CHANGELOG.md" "./CHANGELOG.md")
